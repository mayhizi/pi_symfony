{% extends 'base.html.twig' %}



{% block body %}
<div class="container body">
    <div class="main_container">

        <div class="col-md-3 left_col">
            <div class="left_col scroll-view">
                <div class="navbar nav_title" style="border: 0;">
                    <a href="index.html" class="site_title"><i class="fa fa-paw"></i> <span>Randonn√©e</span></a>
                </div>
            </div>
        </div>

        <div class="right_col" role="main" style="min-height: 2155px;">
            <div class="x_panel">
                <div class="x_title">
                    <h2>{% block pagetitle %}{% endblock %}</h2>
                    <div class="clearfix"></div>
                </div>
                <div class="x_content">
                    {% block content %}{% endblock %}
                </div>
            </div>
            {% block contentBottom %} {% endblock %}
        </div>
    </div>
</div>


{% endblock %}











